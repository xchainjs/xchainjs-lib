    "@xchainjs/xchain-client": "*",
    "@xchainjs/xchain-crypto": "*",
    "@xchainjs/xchain-cosmos-sdk": "*",
    "@xchainjs/xchain-util": "*",
    "cosmjs-types": "0.8.0",
    "bech32": "^1.1.3",
    "bip32": "^2.0.6",
    "protobufjs": "6.11.4",
    "secp256k1": "^5.0.0"
