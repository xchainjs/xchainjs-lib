{
  "name": "@xchainjs/xchain-kujira",
  "version": "0.1.13",
  "description": "Custom Kujira client",
  "keywords": [
    "XChain",
    "Kujira"
  ],
  "author": "XChainJS",
  "homepage": "https://github.com/xchainjs/xchainjs-lib",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:xchainjs/xchainjs-lib.git"
  },
  "scripts": {
