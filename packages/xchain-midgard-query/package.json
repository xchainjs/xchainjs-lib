{
  "name": "@xchainjs/xchain-midgard-query",
  "version": "0.1.14",
  "license": "MIT",
  "description": "Module that is responsible for get data from Midgard API",
  "keywords": [
    "XChain"
  ],
  "author": "THORChain",
  "homepage": "https://github.com/xchainjs/xchainjs-lib#readme",
