  },
  "dependencies": {
    "@xchainjs/xchain-ethereum": "0.31.6",
    "@xchainjs/xchain-kujira": "0.1.12",
    "@xchainjs/xchain-mayachain-query": "0.1.8",
    "@xchainjs/xchain-mayachain": "1.0.1",
    "@xchainjs/xchain-thorchain": "1.0.4",
    "@xchainjs/xchain-wallet": "0.1.7",
    "ethers": "5.6.6"
