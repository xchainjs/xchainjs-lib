{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "lint": {},
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "lib/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "src/**/*.ts",
        "test/**/*.ts"
      ]
    },
    "clean": {
      "cache": false
    }
  }
}